{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "HTTP Server",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/app/server/httpserver/test/http/main.go",
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env",
        },
        {
            "name": "HTTP Server(ddtrace)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/app/server/httpserver/test/http/ddtrace/main.go",
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env",
        },
        {
            "name": "HTTP TLS Server",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/app/server/httpserver/test/http2/main.go",
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env",
        },
        {
            "name": "H2C Server",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/app/server/httpserver/test/h2c/main.go",
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env",
